{
  "name": "learn-graphql",
  "version": "1.0.0",
  "description": "Reedit Clone to learn GraphQL",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "npm-run-all --parallel watch dev",
    "dev": "nodemon --inspect dist/index.js",
    "start": "node dist/index.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "watch": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AryanshMahato/learn-graphql.git"
  },
  "author": "Aryansh Mahato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AryanshMahato/learn-graphql/issues"
  },
  "homepage": "https://github.com/AryanshMahato/learn-graphql#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "pg": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  }
}
